{
	"name": "yap-tally",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"deploy": "npm run build && touch ./build/.nojekyll && echo 'app.patrickjoyce.dev' > ./build/CNAME && npx gh-pages -d build -t true",
		"validate": "prettier --write . && prettier --check . && eslint . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/eslint": "9.6.1",
		"@types/google.maps": "^3.55.9",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.0.0",
		"gh-pages": "^6.1.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.15.0",
		"vite": "^6.2.5"
	},
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.16.6",
		"@googlemaps/adv-markers-utils": "^1.1.0"
	}
}
